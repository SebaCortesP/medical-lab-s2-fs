sonar.projectKey=your-angular-app-key
sonar.projectName=Your Angular App Name
sonar.sources=src
sonar.tests=src
sonar.test.inclusions=**/*.spec.ts
sonar.exclusions=**/node_modules/**, **/dist/**

# Path to the generated LCOV file
sonar.javascript.lcov.reportPaths=coverage/your-project-name/lcov.info
